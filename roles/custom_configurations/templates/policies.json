{
  "policies": {
    "Cookies": {
      "AcceptThirdParty": "always",
      "Default": true,
      "ExpireAtSessionEnd": true,
      "RejectTracker": true
    },
    "DisableFeedbackCommands": true,
    "DisableFirefoxAccounts": true,
    "DisableFirefoxStudies": true,
    "DisableMasterPasswordCreation": true,
    "DisablePocket": true,
    "DisableSetDesktopBackground": true,
    "DisableTelemetry": true,
    "EnableTrackingProtection": {
      "Cryptomining": true,
      "Fingerprinting": true,
      "Value": true
    },
    "Homepage": {
      "StartPage": "none"
    },
    "NewTabPage": false,
    "NoDefaultBookmarks": true,
    "OverridePostUpdatePage": "",
    "PasswordManagerEnabled": false,
    "Permissions": {
      "Camera": {
        "BlockNewRequests": true
      },
      "Location": {
        "BlockNewRequests": true
      },
      "Microphone": {
        "BlockNewRequests": true
      },
      "Notifications": {
        "BlockNewRequests": true
      }
    },
    "SanitizeOnShutdown": {
      "Cache": true,
      "Cookies": true,
      "Downloads": true,
      "FormData": true,
      "History": true,
      "OfflineApps": true,
      "Sessions": true,
      "SiteSettings": true
    },
    "SearchBar": "unified",
    "SearchEngines": {
      "Default": "DuckDuckGo",
      "PreventInstalls": true
    },
    "SearchSuggestEnabled": false,
    "DefaultDownloadDirectory": "/tmp"
  }
}
